$input = null; // Load the input however you want

$inputArray = explode(PHP_EOL, $input);

$numIncreases = 0;
$lastDepth = -1;

foreach ($inputArray as $input) {

  if ($lastDepth === -1) {
    $lastDepth = $input;
    continue;
  } else {
      if (intval($input) > $lastDepth) {
      	++$numIncreases;
      }	
  }

  
  $lastDepth = intval($input);

}

return $numIncreases;